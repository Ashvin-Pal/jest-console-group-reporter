{
  "name": "jest-console-group-reporter",
  "version": "0.18.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "author": "ashvin-pal",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ashvin-Pal/jest-console-group-reporter"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "jest",
    "reporter",
    "console",
    "group"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "test": "jest --colors",
    "test:coverage": "jest --coverage",
    "build": "tsup",
    "lint": "eslint --no-error-on-unmatched-pattern ./src/**/* --ext .ts -c ./.eslintrc.json",
    "format:check": "prettier ./src --check",
    "ci:publish": "pnpm build && pnpm publish --no-git-checks",
    "ci:version": "pnpm changeset version"
  },
  "dependencies": {
    "chalk": "^4.0.0"
  },
  "peerDependencies": {
    "@jest/reporters": ">=25.1.0",
    "jest-message-util": ">=25.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@swc/core": "^1.3.99",
    "@swc/jest": "^0.2.29",
    "@types/jest": ">=25.1.0",
    "@types/node": "^20.10.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": ">=25.1.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "jest-console-group-reporter": "^0.18.0"
  },
  "packageManager": "pnpm@8.9.2"
